@model IEnumerable<Usuario>

@{
    ViewData["Title"] = "Index";
    ViewData["Nav"] = "LISTADO ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br>
<div class="container">

<a asp-action="Create" class="btn btn-outline-primary col-2"><i class="bi bi-person-plus-fill"> Crear Administrador</i></a>
<br>
<br>
  <table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombres)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipo_Usuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombres)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Correo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tipo_Usuario.Tipo)
                </td>
                <td>
                </td>


            </tr>
        }
    </tbody>
</table>
</div>