
        <div id="main-container-registro">
            <div id="form-container">
                
                <form action="*" method="POST">
                    <img src="Logo.png"/>
                    <div class="row" >
                        <div class="col-12 col-md-6">
                            <label for="txtNombreUsuario">Nombre de Usuario</label>
                            <input name="txtNombreUsuario" type="text" class="form-control" placeholder="Nombre de Usuario">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtCorreo">Email</label>
                            <input name="txtCorreo" type="email" class="form-control" placeholder="Email">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtTelefono">Telefono</label>
                            <input name="txtTelefono" type="text" class="form-control" placeholder="Telefono">
                        </div>
                        <div class="col-12 col-md-6">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtNombre">Nombre</label>
                            <input name="txtNombre" type="text" class="form-control" placeholder="Nombre">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtApellido">Apellido</label>
                            <input name="txtApellido" type="text" class="form-control" placeholder="Apellido">
                        </div>
                        
                        <div class="col-12 col-md-6">
                            <label for="txtRut">Rut</label>
                            <input name="txtRut" type="text" class="form-control" placeholder="Rut">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtFechaNacimiento">Fecha de Nacimiento</label>
                            <input name="txtFechaNacimiento" type="date" class="form-control" placeholder="">
                        </div>                        
                        <div class="col-12 col-md-6">
                            <label for="txtContraseña">Contraseña</label>
                            <input name="txtContraseña" type="password" class="form-control" placeholder="Contraseña">
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="txtRepContraseña">Repetir contraseña</label>
                            <input name="txtRepContraseña" type="password" class="form-control" placeholder="Repetir contraseña">
                        </div>
                    </div>
                    <button type="submit" class="btn btn-primary boton-form">Registrarse</button>
                </form>
            </div>
        </div>
