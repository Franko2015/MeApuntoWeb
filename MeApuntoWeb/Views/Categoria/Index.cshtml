
@model List<Categoria>

    <a asp-action="Index" asp-controller="Home"><img class="btnVolver" src="~/return_icon_154912.png" alt="100px"></a>
   
    <div class="container">

  <a asp-action="Create" class="btn btn-outline-primary col-2"><i class="bi bi-person-plus-fill"> Crear Lugar</i></a>
      <br>
      
<br>
  <table class="table table-hover table-bordered" style="color: black;">
  
    <thead>
      <tr>
        <th scope="col">Categoria</th>
        <th scope="col">Acción</th>
      </tr>
    </thead>
    <tbody>
      <tr>
          @foreach (var item in Model)
          {
            <tr>
            <td>@item.categoria</td>
            </tr>
          }
      
    </tbody>
  </table>
</div>
      
